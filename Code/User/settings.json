{
    "window.zoomLevel": 0,
    "workbench.editor.enablePreview": false,
    "editor.tabSize": 2,
    "vim.useCtrlKeys": false,
    "vim.handleKeys": {
        "<C-d>": false
    },
    "beautify.language": {
        "vue": {
            "tab_size": 2,
            "indent_size": 2,
            "ext": ["vue"]
        },
        "js": {
            "tab_size": 2,
            "indent_size": 2,
            "type": [
                "javascript",
                "json"
            ],
            "filename": [
                ".jshintrc",
                ".jsbeautify"
            ]
        },
        "css": [
            "css",
            "scss"
        ],
        "html": [
            "erb",
            "htm",
            "html",
            "Eex",
            "HTML (Eex)"
        ]
    },
    "emmet.includeLanguages": {
        "HTML (Eex)": "html",
        "erb": "html"
    },
    "ruby.codeCompletion": "rcodetools",
    "telemetry.enableTelemetry": false,
    "workbench.colorTheme": "dark+(elixir)",
    "breadcrumbs.enabled": true,
}